<body class=" " ng-app="UberApp" ng-controller="UberCtrl">
    <section class="wrapper">
        <header class="contextual">
            <div class="logo"></div>
        </header>
        <section class="content">
            <div class="signup-wrapper">
                <section class="signup-top">
                    <div class="signup-top-images">
                        <div class="signup-top-phones"></div>
                    </div>
                    <div class="signup-top-text">
                        <div class="signup-top-text-title">
                            Sign Up to Ride
                        </div>
                        <div class="signup-top-text-subtitle">
                            <p>Welcome to Uber, the easiest way to get around at the tap of a button.</p>
                            <p>Create your account and get moving in minutes.</p>
                        </div>
                    </div>
                </section>
                <section class="signup-form">
                    <article   autocomplete="on" class="form-vertical" >
                            <div class="signup-form-section account">
                                <div class="signup-form-section-title">
                                    <p>Account</p>
                                </div>
                                <div class="signup-form-section-required">
                                    Required
                                </div>
                                <div class="signup-form-section-number blue">
                                    1
                                </div>
                                <div class="signup-form-section-line"></div>
                                <div class="form-group required">
                                    <label for="email">Email</label> <input id="email" name="email" placeholder="name@example.com" type=
                                    "email" value="" ng-model="email"
                                    >
                                    
                                </div>
                                <div class="form-group required">
                                    <label for="password">Password</label> <input id="password" name="password" placeholder=
                                    "At least 5 characters" type="password" value="" ng-model="password">
                                    
                                </div>
                            </div>
                            <div class="signup-form-section profile">
                                <div class="signup-form-section-title">
                                    <p>Profile</p>
                                </div>
                                <div class="signup-form-section-number">
                                    2
                                </div>
                                <div class="signup-form-section-line"></div>
                                <div class="form-group required">
                                    <label for="name">Name</label>
                                    <div class="row form-row">
                                        <div class="col-md-6 col-xs-6 md-push-right-half sm-push-right-half xs-push-right-half">
                                            <input id="first_name" name="first_name" placeholder="First Name" type="text" value="" ng-model="first_name">
                                            <div class="error-notice"></div>
                                        </div>
                                        <div class="col-md-6 col-xs-6 md-push-left-half xs-push-left-half">
                                            <input id="last_name" name="last_name" placeholder="Last Name" type="text" value="" ng-model="last_name">
                                            <div class="error-notice"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group js-mobile-country required mobile-number">
                                    <label for="mobile">Mobile Number</label> <input id="mobile" name="mobile" placeholder="(201) 555-5555"
                                    type="tel" value="" ng-model="mobile">
                                </div>
                                <div class="form-group required">
                                    <label for="language">Language</label> <input id="lang" name="lang" placeholder="English" type=
                                    "text" value="" ng-model="lang">
                                </div>
                            </div>
                            <div class="signup-form-section payment">
                                <div class="signup-form-section-title">
                                    <p>Payment</p>
                                </div>
                                <div class="signup-form-section-number">
                                    3
                                </div>
                                <div class="signup-form-section-line"></div>
                                <div class="credit-card-info">
                                    <div class="row form-row">
                                        <div class="col-md-9 col-xs-9 xs-push-right-half">
                                            <div class="form-group required">
                                                <label for="card_number">Credit Card Number</label> <input id="card_number" name=
                                                "card_number" placeholder="1234 5678 9012 3456" type="tel" value="" ng-model="card_number">
                                                <div class="credit-card-icon"></div>
                                            </div>
                                        </div>
                                        <div class="col-md-3 col-xs-3 xs-push-left-half">
                                            <div class="form-group required">
                                                <label for="card_code">CVV</label> <input id="card_code" name="card_code" placeholder="123"
                                                type="tel" value="" ng-model="card_code">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group required">
                                        <div class="row form-row">
                                            <div class="col-md-8 col-xs-12 full-row-xs">
                                                <label for="card_expiration">Expiration Date</label>
                                                <div class="row form-row">
                                                    <div class="col-md-6 col-xs-6 xs-push-right-half col-xs-row">
                                                        <input id="card_expiration_month" name="card_expiration_month" placeholder="01"
                                                type="number" value="" ng-model="card_expiration_month">
                                                        
                                                    </div>
                                                    <div class="col-md-6 col-xs-6 xs-push-left-half col-xs-row">
                                                     <input id="card_expiration_year" name="card_expiration_year" placeholder="2015"
                                                type="number" value="" ng-model="card_expiration_year">   
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-xs-12">
                                                <label for="billing_zip">Postal Code</label> <input id="billing_zip" name="billing_zip"
                                                placeholder="94103" type="text" value="" ng-model="billing_zip">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="add-a-promo-code">
                                    Add a Promo Code
                                </div>
                                <div class="signup-form-section submit">
                                    <div class="col-md-12">
                                        <img class="loader defer_image_loading" data-src=
                                        "https://d1a3f4spazzrp4.cloudfront.net/free-candy/images/gfx/loader.a5b022ab0fad102f521a454df3ceb117.gif"
                                        src=
                                        "https://d1a3f4spazzrp4.cloudfront.net/free-candy/images/gfx/loader.a5b022ab0fad102f521a454df3ceb117.gif">
                                        <div class="form-group">
                                            <button class="btn btn-primary js-submit" type="submit" ng-click="signup();">Create Account</button>
                                        </div>
                                        <div class="signup-form-section-notice">
                                            Please fill out all required (<span class="star">*</span>) fields.
                                        </div>
                                    </div>
                                    <div class="signup-form-section-number"></div>
                                </div>
                                <div class="form-group agree-terms">
                                    <p>By clicking Create Account , you agree to Uber's <a href="https://www.uber.com/legal/terms">Terms
                                    and Conditions</a> and <a href="https://www.uber.com/legal/privacy">Privacy Policy</a>.</p>
                                </div>
                            </div>
                        </form>
                    </article>
                </section>
            </div>
        </section>
    </section>
    
</body>
